{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "standard"
    ],
    "parserOptions": {
        "ecmaVersion": 12,
        "sourceType": "module"
    },
    "rules": {
      "quotes": "off",
      "no-unreachable-loop": "off",
      "no-throw-literal": "warn"
    }
}
